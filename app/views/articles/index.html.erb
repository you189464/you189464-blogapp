<div class="container">
  <%= render 'tabs' %>

  <div class="contents">
    <% @articles.each do |article| %>
      <%= render 'article', article: article %>
    <% end %>
  </div>

  <%= link_to new_article_path do %>
    <div class="float_btn">
      <i class="fa fa-plus"></i>
    </div>
  <% end %>

</div>